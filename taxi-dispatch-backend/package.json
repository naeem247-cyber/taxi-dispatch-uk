    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/websockets": "^10.4.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "ioredis": "^5.4.1",
    "passport": "^0.7.0",
